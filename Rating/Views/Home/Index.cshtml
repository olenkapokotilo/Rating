@model IEnumerable<Rating.Models.UserModel>
    <div class="col-md">
    <h3>Users</h3>
        <table class="table table-hover">
            <tr>
                <td><p>Email</p></td>
                <td></td>
            </tr>
            @foreach (var u in Model)
            {
                <tr>
                    <td><p>@u.Email    </p></td>
                    <td><a href="/Home/About/@u.Id">About</a></td>
                </tr>
            }
        </table>
</div>

